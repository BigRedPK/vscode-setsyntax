{
    "name": "set-syntax",
    "displayName": "Set Syntax",
    "description": "Sublimes Set Syntax alias for Language Mode",
    "version": "0.0.1",
    "publisher": "NA",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.setSyntax"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.setSyntax",
            "title": "Set Syntax"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}